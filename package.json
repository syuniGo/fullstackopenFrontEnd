{
  "name": "exercises",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docker": "cd ../backend/ && docker build -t gcr.io/gcp-stock-analytics/my-app .",
    "build:ui": "rm -rf dist && cd ../frontend/ && npm run build && cp -r dist ../backend",
    "build:full": "npm run build:ui && npm run build:docker",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "json-server -p3001 --watch db.json",
    "deploy": "sh ../backend/script/deploy.sh",
    "deploy:full": "npm run build:full && npm run deploy",
    "logs:prod": "fly logs"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
